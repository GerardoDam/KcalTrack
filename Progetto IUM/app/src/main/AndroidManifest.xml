<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kcaltrack"
    >

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo1"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.NoActionBar">
        <activity android:name=".information_page"></activity>
        <activity android:name=".popupCibi" android:theme="@style/Theme.KcalTrack.CustomTheme"></activity>
        <activity android:name=".popupEse" android:theme="@style/Theme.KcalTrack.CustomTheme"></activity>
        <activity android:name=".popupAcqua" android:theme="@style/Theme.KcalTrack.CustomTheme"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>